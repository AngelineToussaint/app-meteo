{
  "extends": "../tslint.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": [
          "jquery",
         "materialize-css"
  ]
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ],
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
